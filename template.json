{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accounts_cv_docucheck_name": {
            "defaultValue": "cv-docucheck",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.CognitiveServices/accounts",
            "apiVersion": "2024-06-01-preview",
            "name": "[parameters('accounts_cv_docucheck_name')]",
            "location": "eastus",
            "sku": {
                "name": "S1"
            },
            "kind": "ComputerVision",
            "identity": {
                "type": "None"
            },
            "properties": {
                "customSubDomainName": "[parameters('accounts_cv_docucheck_name')]",
                "networkAcls": {
                    "defaultAction": "Allow",
                    "virtualNetworkRules": [],
                    "ipRules": []
                },
                "publicNetworkAccess": "Enabled"
            }
        }
    ]
}